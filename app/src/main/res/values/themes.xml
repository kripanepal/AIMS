<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="Aims_delivery" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="colorPrimary">@color/Aims_Orange</item>
        <item name="colorPrimaryVariant">@color/Aims_light_orange</item>
        <item name="colorOnPrimary">@color/black</item>
        <item name="color">#EAE8E5</item>
        <item name="colorSecondary">@color/card_color</item>
        <item name="colorSecondaryVariant">@color/card_background_color</item>
        <item name="colorOnSecondary">@color/black</item>
        <item name="colorAccent">@color/Aims_Orange</item>
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <item name="android:textColorPrimary">?attr/colorOnPrimary</item>
    </style>


    <style name="Aims_delivery.splash_skin" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="android:windowBackground">@drawable/splash_image</item>
        <item name="android:statusBarColor">@color/Aims_Orange</item>
    </style>


    <style name="textColorLight">
        <item name="android:textColor">#99716F6F</item>
    </style>


</resources>
