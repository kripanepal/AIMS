<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="Aims_delivery" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="colorPrimary">@color/Aims_Orange</item>
        <item name="colorPrimaryVariant">@color/Aims_light_orange</item>
        <item name="colorOnPrimary">@color/light_grey</item>
        <item name="color">@color/ExtraLightblack_card_color</item>
        <item name="colorSecondary">@color/LightBlack_card_color</item>
        <item name="colorSecondaryVariant">@color/black</item>
        <item name="colorOnSecondary">@color/white</item>
        <item name="colorAccent">@color/Aims_Orange</item>
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimary</item>
        <item name="android:textColorPrimary">?attr/colorOnPrimary</item>

    </style>


    <style name="Aims_delivery.splash_skin" parent="Theme.MaterialComponents.Light.NoActionBar">
